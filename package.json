{
  "name": "package-skeleton",
  "version": "1.0.4",
  "description": "Skeleton of a simple NPM Package with Babel and CI already configured.",
  "license": "ISC",
  "repository": "amercier/npm-package-skeleton",
  "author": "Alex Mercier <pro.alexandre.mercier@gmail.com> (http://amercier.com)",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "transpile": "rimraf dist && babel src -d dist",
    "prepublish": "npm run transpile",
    "doc": "rimraf esdoc && esdoc -c esdoc.json",
    "lint": "eslint --ignore-path .gitignore .",
    "start": "export NODE_ENV=development; npm run test -- --watch --notify",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.js",
  "keywords": [
    "package",
    "skeleton",
    "example",
    "node",
    "npm",
    "babel",
    "ci",
    "code",
    "codeclimate",
    "codecov",
    "coverage",
    "eslint",
    "gemnasium",
    "jest"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.2",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "jest": "^23.2.0",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": ["lcov", "text", "html"]
  }
}
