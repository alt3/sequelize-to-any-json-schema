{
  "name": "@alt3/sequelize-to-json-schemas",
  "version": "0.0.1",
  "description": "Convert Sequelize models to various JSON Schema variants using the Strategy Pattern.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:alt3/sequelize-to-json-schemas.git"
  },
  "author": {
    "name": "ALT3 B.V.",
    "url": "https://github.com/alt3/sequelize-to-json-schemas"
  },
  "contributors": [],
  "bugs": "https://github.com/alt3/sequelize-to-json-schemas/issues",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "docs": "rimraf docs && npx jsdoc -c .jsdoc",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:prettier": "prettier --list-different '**/*.{js,json,md,yml}'",
    "lint": "run-p lint:*",
    "start": "npx nodemon index.js",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "keywords": [
    "javascript",
    "sequelize",
    "json-schema",
    "strategy-pattern",
    "swagger",
    "openapi",
    "oas",
    "oasv3"
  ],
  "dependencies": {
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "acorn": "^6.2.1",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsdoc": "^15.8.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-unicorn": "^8.0.0",
    "husky": "^2.0.0",
    "jest": "^23.6.0",
    "jsdoc": "^3.6.3",
    "lint-staged": "^8.0.4",
    "mysql2": "^1.6.5",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.15.1",
    "rimraf": "^2.6.2",
    "sequelize": "^5.12.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --list-different",
    "*.js": [
      "eslint",
      "jest --bail --findRelatedTests"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:jsdoc/recommended",
      "plugin:prettier/recommended",
      "plugin:unicorn/recommended",
      "prettier",
      "prettier/unicorn"
    ],
    "env": {
      "browser": false
    },
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "env": {
          "jest": true
        },
        "rules": {
          "import/no-extraneous-dependencies": [
            "error",
            {
              "devDependencies": true
            }
          ]
        }
      }
    ]
  },
  "esdoc": {
    "source": "./src",
    "destination": "./esdoc",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  }
}
